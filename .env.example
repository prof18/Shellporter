# Copy to .env and fill in your values. The .env file is gitignored.

# Code signing identity (e.g., "Developer ID Application: Your Name (TEAMID)")
APP_IDENTITY=

# Apple ID email for notarization
APPLE_ID=

# Apple Developer Team ID
TEAM_ID=

# Keychain profile name for notarization credentials (default: NOTARIZATION_PASSWORD)
KEYCHAIN_PROFILE=NOTARIZATION_PASSWORD

